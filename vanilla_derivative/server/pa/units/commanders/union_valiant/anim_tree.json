                    ]
                }
            },
            "dead": {
                "type": "bind_pose"
            },
            "ghost": {
                "type": "bind_pose"
            }
        }
    },
    "skeleton_controls": [
        {
            "type": "fader",
            "lerp_func": "is_active",
            "child": {
                "type": "procedural_aim",
                "rotation_bone": "bone_turret",
                "rotation_axis": "z"
            }
        },
        {
            "type": "fader",
            "lerp_func": "is_active",
            "child": {
                "type": "procedural_aim",
                "rotation_bone": "bone_turret_pitch",
                "rotation_axis": "x"
            }
        }
    ]
}