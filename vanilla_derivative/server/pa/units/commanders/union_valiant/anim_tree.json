{
    "blend_root": {
        "type": "unit_state",
        "nodes": {
            "being_built": {
                "type": "bind_pose"
            },
            "living": {
                "type": "aim_blend",
                "aim_bone_root": "bone_turret",
                "node_aim_down": {
                    "type": "playback",
                    "anim_name": "aim_down"
                },
                "node_aim_neutral": {
                    "type": "bind_pose"
                },
                "node_aim_up": {
                    "type": "playback",
                    "anim_name": "aim_up"
                },
                "child": {
                    "type": "sequence",
                    "entries": [
                        {
                            "type": "item",
                            "name": "idle",
                            "node": {
                                "type": "playback",
                                "anim_name": "idle",
                                "looping": true,
                                "events": []
                            },
                            "transitions": [
                                {
                                    "transition_forward": {
                                        "func": "is_moving",
                                        "target": "walking",
                                        "duration": 0.2
                                    },
                                    "transition_reverse": {
                                        "func": "is_moving",
                                        "target": "walking",
                                        "duration": 0.2
                                    }
                                }
                            ],
                            "reset": "playback_reset"
                        },
                        {
                            "type": "item",
                            "name": "walking",
                            "scale_to_move_speed": true,
                            "node": {
                                "type": "playback",
                                "anim_name": "walk",
                                "looping": true,
                                "events": [
                                    {
                                        "time": 0.46,
                                        "type": "audio",
                                        "parameters": {
                                            "cue": "/SE/Movement/commander/Commander_step"
                                        }
                                    },
                                    {
                                        "time": 0.46,
                                        "type": "effect",
                                        "parameters": {
                                            "bone": "bone_right_toe",
                                            "spec": "/pa/effects/specs/commander_step.pfx",
                                            "effect_scale": 1.0,
                                            "bone_offset": [
                                                0.0,
                                                0.0,
                                                0.0
                                            ]
                                        }
                                    },
                                    {
                                        "time": 1.06,
                                        "type": "audio",
                                        "parameters": {
                                            "cue": "/SE/Movement/commander/Commander_step"
                                        }
                                    },
                                    {
                                        "time": 1.06,
                                        "type": "effect",
                                        "parameters": {
                                            "bone": "bone_left_toe",
                                            "spec": "/pa/effects/specs/commander_step.pfx",
                                            "effect_scale": 1.0,
                                            "bone_offset": [
                                                0.0,
                                                0.0,
                                                0.0
                                            ]
                                        }
                                    }
                                ]
                            },
                            "transitions": [
                                {
                                    "transition_forward": {
                                        "func": "not_moving",
                                        "target": "walk_stop",
                                        "duration": 0.4
                                    },
                                    "transition_reverse": {
                                        "func": "not_moving",
                                        "target": "walk_stop",
                                        "duration": 0.4
                                    }
                                }
                            ],
                            "reset": "playback_reset"
                        },
                        {
                            "type": "item",
                            "name": "walk_stop",
                            "node": {
                                "type": "playback",
                                "anim_name": "walk_stop",
                                "looping": false,
                                "events": [
                                    {
                                        "time": 0.36,
                                        "type": "audio",
                                        "parameters": {
                                            "cue": "/SE/Movement/commander/Commander_step"
                                        }
                                    },
                                    {
                                        "time": 0.95,
                                        "type": "audio",
                                        "parameters": {
                                            "cue": "/SE/Movement/commander/Commander_step"
                                        }
                                    },
                                    {
                                        "time": 1.33,
                                        "type": "audio",
                                        "parameters": {
                                            "cue": "/SE/Movement/commander/Commander_step"
                                        }
                                    }
                                ]
                            },
                            "transitions": [
                                {
                                    "transition_forward": {
                                        "func": "anim_complete",
                                        "target": "idle"
                                    },
                                    "transition_reverse": {
                                        "func": "anim_complete",
                                        "target": "idle"
                                    }
                                },
                                {
                                    "transition_forward": {
                                        "func": "is_moving",
                                        "target": "walking",
                                        "duration": 0.2
                                    },
                                    "transition_reverse": {
                                        "func": "is_moving",
                                        "target": "walking",
                                        "duration": 0.2
                                    }
                                }
                            ],
                            "reset": "playback_reset"
